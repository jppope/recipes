			"type": "string"
		},
		"ingredients": {
			"type": "array",
			"minItems": 1,
			"items": [
				{
					"type": "object",
					"properties": {
						"count": {
							"description": "ingredient measurement\n e.g. (1) apple or 0.5 cups\n",
							"type": "number"
						},
						"unit": {
							"description": "measurement unit\ne.g. ounces, cups, kilograms, etc\n",
